<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="MChat">
  <grid id="27dc6" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="919" height="711"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <!-- Nachrichten-Bereich (Mitte) -->
      <scrollpane id="messageScrollPane" class="javax.swing.JScrollPane">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="b4e6e" class="javax.swing.JTextPane" binding="messageReceiver">
            <constraints/>
            <properties>
              <editable value="false"/>
              <text value=""/>
            </properties>
          </component>
        </children>
      </scrollpane>
      
      <!-- Unterer Bereich fÃ¼r Eingabe und Buttons -->
      <grid id="bottomPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <!-- Eingabe-Panel (Mitte des unteren Bereichs) -->
          <grid id="inputPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="ba9fc" class="javax.swing.JTextField" binding="messageField">
                <constraints/>
                <properties>
                  <columns value="30"/>
                </properties>
              </component>
              <component id="f69d9" class="javax.swing.JButton" binding="sendButton">
                <constraints/>
                <properties>
                  <text value="Senden"/>
                </properties>
              </component>
            </children>
          </grid>
          
          <!-- Button-Panel (Unten) -->
          <grid id="buttonPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="e4f9a" class="javax.swing.JButton" binding="newChatButton">
                <constraints/>
                <properties>
                  <text value="Neuer Chat"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
